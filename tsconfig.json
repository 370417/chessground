{
  "include": [
    "src/*.ts"
  ],
  "filesGlob": [
    "src/*.ts"
  ],
  "exclude": [],
  "compilerOptions": {
    "noEmitOnError": false,
    "outDir": "./",
    "declaration": true,
    "sourceMap": true,
    "removeComments": true,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "module": "commonjs",
    "target": "ES2016",
    "lib": ["DOM", "ES2016"]
  }
}
